<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/button"
        android:layout_width="351dp"
        android:layout_height="49dp"
        android:layout_marginEnd="28dp"
        android:backgroundTint="#A8CAEA"
        android:text="Показать результаты"
        app:layout_constraintEnd_toEndOf="parent"
        tools:layout_editor_absoluteY="598dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="134dp"
        android:layout_height="139dp"
        app:srcCompat="@drawable/image_3"
        tools:layout_editor_absoluteX="228dp"
        tools:layout_editor_absoluteY="174dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="153dp"
        android:layout_height="123dp"
        app:srcCompat="@drawable/image_4"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="381dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="154dp"
        android:layout_height="139dp"
        app:srcCompat="@drawable/image_5"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="174dp" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/image_6"
        tools:layout_editor_absoluteX="228dp"
        tools:layout_editor_absoluteY="381dp" />

</androidx.constraintlayout.widget.ConstraintLayout>